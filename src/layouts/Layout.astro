---
import Head from './partials/Head.astro';
import Header from './partials/Header.astro';
import Footer from './partials/Footer.astro';

import '../styles/style.scss';

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <Head title={title} />
  <Header />
  <slot />
  <Footer />
</html>
